// Two-Pass Compilation Demo
// Demonstrates that functions and classes can be used before they're defined

pub func main() -> i64 {
    // Test 1: Call functions defined later
    var result1 -> i64 = add(5, 3);
    var result2 -> string = greet("World");
    
    // Test 2: Use classes defined later
    var obj -> MyClass = new MyClass("Test");
    
    return result1;
}

// Functions defined after they're used
pub func add(a: i64, b: i64) -> i64 {
    return a + b;
}

pub func greet(name: string) -> string {
    return "Hello, " + name + "!";
}

// Class defined after it's used
pub class MyClass -> Object {
    pub var name -> string;
    
    constructor(name: string) {
        self.name = name;
    }
    
    pub func getName() -> string {
        return name;
    }
} 