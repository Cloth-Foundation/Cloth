mod four

pub func main(argc: []i32, argv: []string): i32 {
    println("=== Array Metadata Demo ===")
    let numbers: []i32 = [10, 20, 30, 40, 50]
    printf("Array length: {}\n", numbers::LENGTH)

    let words: [][]string = [["hello"], ["world", "test"]]
    loop x in words {
        printf("Inner array length: {}\n", x::LENGTH)
        loop i in range(0, x::LENGTH) {
            printf("Word: {}\n", x[i])
        }
    }

    let r: string = "Hello World"
    println(r::LENGTH)

    ret 0
}