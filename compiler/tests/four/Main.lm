mod four

pub func main(argc: []i32, argv: []string): i32 {
    loop x in range(0, 10) {
        println(x)
        if x == 5 {
            println("Breaking the loop!")
            break
        } else {
            println("Continuing the loop...")
            continue
        }
    }

    let iter = 0
    do {
        if iter == 3 {
            break
        }
        printf("This is a do-while loop #{}\n", iter)
        iter++
    } while (iter < 5)

    println(iter)

    while true {
        if iter >= 0 && iter != 1{
            printf("Working! {}\n", iter)
            iter--
            continue
        } elif iter == 1 {
            printf("Iter is {}, exiting...\n", iter)
            break
        }
    }
    ret 0
}