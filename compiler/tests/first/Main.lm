mod first

class Person {
    priv var name: string

    pub Person(n: string) {
        self.name = n
    }

    pub func greet(): void {
        println("hi, I am " + self.name)
    }

    pub func get(): self {
        ret self
    }
}

pub func main(argc: []i32, argv: []string): i32 {
    var one: Person = new Person("onetwo")
    var two: Person = one.get()
    two.greet()
    ret 0x0
}