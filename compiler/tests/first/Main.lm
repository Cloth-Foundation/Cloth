mod first

import first.Human::{ Human, Color }

enum Type {
    HUMAN("Human"),
    DOG("Dog"),
    CAT("Cat")

    priv fin     val: string
    pub Type(val: string) {
        self.val = val
    }
    pub func toString(): string {
        ret self.val
    }
}

pub template class Animal {

    pub Animal() {

    }

    pub template var getType(): Type

}

pub func main(argc: []i32, argv: []string): i32 {
    ret Main()
}

pub func Main(): i32 {
    var alice: Human = new Human("Alice", 21, Color.BLUE)
    var bob: Human = new Human("Bob", 23, Color.RED)
    alice.greet()
    bob.greet()

    if alice.type() == bob.type() {
        println("Alice and Bob are of the same type.")
    } else {
        println("Alice and Bob are of different types.")
    }

    if alice.getAge() > bob.getAge() {
        println("Alice is older than Bob.")
    } elif alice.getAge() < bob.getAge() {
        println("Bob is older than Alice.")
    } else {
        println("Alice and Bob are the same age.")
    }

    # Flip alice and bob values
    let temp = alice
    alice = bob
    bob = temp
    alice.greet()
    bob.greet()

    ret 0
}